{
  "name": "jsdecena/baserepo",
  "description": "Base repository",
  "autoload": {
    "classmap": [
      "tests/database/seeds",
      "tests/database/factories"
    ],
    "psr-4": {
      "Jsdecena\\Baserepo\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jsdecena\\Baserepo\\Test\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.6.0 <9.0",
    "league/fractal": "^0.19.0",
    "illuminate/support": "^5.5|^5.6|^5.7|^5.8|^5.9|^6.0|^7.0|^8.0|^9.0",
    "ext-json": "*"
  },
  "require-dev": {
    "orchestra/testbench": "~3.0",
    "phpunit/phpunit": "^7.5"
  },
  "repositories": {
    "dev-package": {
      "type": "path",
      "url": "src",
      "options": {
        "symlink": true
      }
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Jeff Simons Decena",
      "email": "jeff.decena@yahoo.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Jsdecena\\Baserepo\\BaseRepositoryProvider"
      ]
    }
  }
}
